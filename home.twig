{% extends "layout.twig" %}

{% block content %}

 <header class="bk-header">
   <div class="wrapper clearfix">
        <div class="companyheader clearfix">
            {{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
            {{widget('companyname', 'companyname', {"business": "profile"})|raw}}
        </div>
            {{widget('extendednavigation', 'navigationheader', {"align":"right"})|raw}}
    </div>

</header>

<div class="canvas">

<section>
{{widget('responsiveslideshow', 'thisunqiuewidgetname', {'duration':'3', 'transSpeed': '1000', 'transType': 'fade', 'showButtons': true, 'showBullets': false, 'allowPause': true, 'allowLoop': true, 'images':
[
{'imageHeight':'555', 'imageWidth':'957', 'url':'images/feature-bg1.jpg', 'title':'test title', 'description': 'test desc'},
{'imageHeight':'555', 'imageWidth':'957', 'url':'images/feature-bg2.jpg', 'title':'test title', 'description': 'test desc'},
{'imageHeight':'555', 'imageWidth':'957', 'url':'images/feature-bg3.jpg', 'title':'test title', 'description': 'test desc'}
]
})|raw}}
  
  </section>

<div class="inner-bg">

<div class="main wrapper clearfix">
{{zone('main')|raw}}
</div>  

</div>

</div>


<footer class="bk-footer">
    <div class="wrapper clearfix">
        {{widget('profile', 'footercolumn1socialicons', { 'profileType' : 'socialicons' })|raw}}           
        {{widget('profile', 'footercolumn1address', { 'profileType':'address', 'address':'profile'})|raw}}
        {{widget('profile', 'footercolumn1profilePhone', { 'profileType':'phone', 'phone':'profile'})|raw}} 
    </div>
</footer>



{% endblock %}